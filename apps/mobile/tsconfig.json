      "@act/data/rn": ["../../libs/data/rn/src"],
      "@act/data/core": ["../../libs/data/core/src"]
    },
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "exclude": ["node_modules"]
}
